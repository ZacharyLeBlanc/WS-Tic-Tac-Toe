{
  "name": "client",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.svelte --fix",
    "prettier": "prettier --write src/*.js src/**/*.js src/*.svelte src/**/*.svelte",
    "precommit": "lint-staged && yarn run test:ci"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@rollup/plugin-replace": "^2.3.0",
    "@testing-library/jest-dom": "^5.0.0",
    "@testing-library/svelte": "^1.11.0",
    "babel-jest": "^25.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "jest": "^25.1.0",
    "jest-transform-svelte": "^2.1.1",
    "lint-staged": "^10.0.1",
    "prettier": "^1.19.1",
    "prettier-plugin-svelte": "^0.7.0",
    "rollup": "^1.20.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.1.2",
    "svelte": "^3.0.0"
  },
  "dependencies": {
    "@material/drawer": "^4.0.0",
    "@material/snackbar": "^4.0.0",
    "@material/top-app-bar": "^4.0.0",
    "dotenv": "^8.2.0",
    "sirv-cli": "^0.4.4",
    "socket.io": "^2.3.0",
    "svelte-routing": "^1.4.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,svelte}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
